{
  "name": "todo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "firebase": "^6.1.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-flexbox-grid": "^2.1.2",
    "react-moment": "^0.9.2",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-with-firebase-auth": "^1.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "storybook-router": "^0.3.4"
  },
  "scripts": {
    "start": "NODE_ENV=development concurrently --kill-others-on-fail \"npm run server:start\" \"npm run client:start\"",
    "clean": "rimraf dist",
    "client:start": "react-scripts start",
    "client:build": "react-scripts build",
    "client:test": "react-scripts test",
    "client:eject": "react-scripts eject",
    "server:start": "babel-node src/App/Server/bin/www",
    "server:prod": "node dist/application/server/bin/www",
    "server:build": "babel src --ignore src/application/client --out-dir dist",
    "server:deploy": "NODE_ENV=production npm-run-all clean server:build client:build server:prod",
    "storybook": "start-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "concurrently": "^4.1.0"
  }
}
